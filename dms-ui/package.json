{
  "name": "dms-ui",
  "version": "v1.1.1-SNAPSHOT",
  "private": true,
  "description": "Dms ui application",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 max build",
    "build": "cross-env REACT_APP_ENV=pre UMI_ENV=pre max build",
    "deploy": "npm run build && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "max setup",
    "jest": "jest",
    "lint": "npm run lint:js && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src ",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"**/**.{js,jsx,tsx,ts,less,md,json}\" --end-of-line auto",
    "openapi": "max openapi",
    "prepare": "husky",
    "prettier": "prettier -c --write \"**/**.{js,jsx,tsx,ts,less,md,json}\"",
    "preview": "npm run build && max preview --port 8000",
    "record": "cross-env NODE_ENV=development REACT_APP_ENV=test max record --scene=login",
    "serve": "umi-serve",
    "start": "cross-env REACT_APP_ENV=dev UMI_ENV=dev max dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev max dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev max dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=pre max dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev max dev",
    "test": "jest",
    "test:coverage": "npm run jest -- --coverage",
    "test:update": "npm run jest -- -u",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ag-grid-community/client-side-row-model": "^31.1.1",
    "@ag-grid-community/core": "^31.1.1",
    "@ag-grid-community/react": "^31.1.1",
    "@ag-grid-community/styles": "^31.1.1",
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-components": "^2.3.47",
    "@ant-design/pro-layout": "^7.10.3",
    "@ant-design/use-emotion-css": "1.0.4",
    "@ant-design/x": "^1.0.6",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@monaco-editor/react": "^4.4.6",
    "@types/babel__template": "^7.4.1",
    "@umijs/route-utils": "^2.1.3",
    "antd": "^5.25.2",
    "antd-style": "^3.7.1",
    "copy-to-clipboard": "^3.3.3",
    "file-saver": "^2.0.5",
    "idb": "^8.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "monaco-editor": "^0.37.0",
    "monaco-themes": "^0.4.4",
    "omit.js": "^2.0.2",
    "rc-menu": "^9.6.4",
    "rc-util": "^5.24.4",
    "react": "^18.0.0",
    "react-activation": "^0.12.4",
    "react-cookies": "^0.1.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dev-inspector": "^1.8.1",
    "react-dom": "^18.0.0",
    "react-helmet-async": "^1.3.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^2.0.19",
    "react-syntax-highlighter": "^15.6.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "socket.io-client": "^4.7.2",
    "sql-formatter": "^15.3.1",
    "tailwindcss": "^4.1.1",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.0",
    "@tailwindcss/postcss": "^4.1.0",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/react": "^13.4.0",
    "@types/express": "^4.17.14",
    "@types/file-saver": "^2.0.7",
    "@types/history": "^4.7.11",
    "@types/jest": "^29.2.1",
    "@types/lodash": "^4.14.186",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-helmet": "^6.1.5",
    "@umijs/lint": "^4.0.34",
    "@umijs/max": "^4.0.33",
    "@umijs/plugins": "^4.4.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "express": "^4.18.2",
    "gh-pages": "^3.2.0",
    "husky": "^9",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "lint-staged": "^13",
    "mockjs": "^1.1.0",
    "monaco-editor-webpack-plugin": "^7.0.1",
    "postcss": "^8.5.3",
    "prettier": "^2",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-packagejson": "^2.4.3",
    "swagger-ui-dist": "^4.14.2",
    "ts-node": "^10.9.1",
    "typescript": "^5",
    "umi-presets-pro": "^2.0.0"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
